<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="clinical_report_pdf_inherited" model="ir.ui.view">
            <field name="name">historial.clinico.report.form</field>
            <field name="model">historial.clinico</field>
            <field name="inherit_id" ref="ofimatica_clinical_management.historial_clinico_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet" position='before'>
                    <header>
                        <button name="%(ofimatica_clinical_management.project_report_open_wizard)d" type="action" string="Print" class="oe_highlight"/>
                    </header>
                </xpath>
            </field>
       </record>
    </data>
</odoo>